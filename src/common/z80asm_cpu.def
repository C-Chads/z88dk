#ifndef X
#define X(id, value, name)
#endif

// order specific to generic, so that the specific ones are included first
// in libaries when using the -m'*' option
// see #2320
X(CPU_GBZ80, 1 << 9, "gbz80")

X(CPU_EZ80, 1 << 3, "ez80")

X(CPU_R3K, 1 << 6, "r3k")
X(CPU_R2KA, 1 << 5, "r2ka")

X(CPU_Z80N, 1 << 1, "z80n")
X(CPU_Z80, 1 << 0, "z80")
X(CPU_EZ80_Z80, 1 << 4, "ez80_z80")
X(CPU_Z180, 1 << 2, "z180")

X(CPU_8085, 1 << 8, "8085")
X(CPU_8080, 1 << 7, "8080")

#undef X
